{
  "name": "eslintFixer",
  "displayName": "ESLint Fixer",
  "description": "A VSCode extension to fix ESLint errors.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.fixESLintErrors"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.fixESLintErrors",
        "title": "Fix ESLint Errors"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security"
  }
}

